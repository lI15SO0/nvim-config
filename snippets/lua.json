{
	"Neovim Module init": {
		"prefix": "nmi",
		"body": [
			"local api = require(\"api\")\n",
			"local path=\"$1\"",
			"local plugins = {",
			"$0",
			"}",
			"return api.plugin.construct(path, plugins)"
		],
		"description": "Plugins that not require anything's template"
	},
	"New Module": {
		"prefix": "mod",
		"body": [
			"local M = {}",
			"",
			"function M.$1()",
			"$0",
			"end",
			"",
			"return M"
		],
		"description": "New local M module snip."
	}
}
